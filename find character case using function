#include<iostream>
using namespace std ;


 char  findcharatcetr( char ch){

  if (ch >= 'A' && ch<='Z')
  {
     cout << "1";
  }
  else if (ch>='a' && ch<='z')
  {
      cout << "0";
  }
   else {
      cout << "-1"; 
   }
 }
int main () {

     char ch;
     cin >> ch ;
     findcharatcetr(ch);
}
